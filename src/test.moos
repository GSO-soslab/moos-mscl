// MOOS file

ServerHost = localhost
ServerPort = 9000

ProcessConfig = ANTLER
{
    MSBetweenLaunches = 200
    Run = pMicrostrainIMU @ NewConsole = false
}

ProcessConfig = pMicrostrainIMU
{
    AppTick   = 4
    CommsTick = 4

    port = "/dev/ttyACM0"
    baudrate = 115200

    prefix = "MS"

    publish_raw = TRUE
    publish_filter = TRUE

    publish_frequency = 40
    imu_frequency = 40
    filter_frequency = 40

}
